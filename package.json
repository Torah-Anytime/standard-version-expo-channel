{
    "name": "@torah-anytime/standard-version-expo-channel",
    "version": "0.1.0",
    "description": "Set Expo Publish channel name to version",
    "scripts": {
        "build": "tsc",
        "release": "standard-version",
        "release:major": "mpm run release -- -r major",
        "release:minor": "npm run release -- -r minor",
        "release:patch": "npm run release -- -r patch"
    },
    "files": [
        "dist/*"
    ],
    "dependencies": {
        "@types/node": "^16.9.1",
        "plist": "^3.0.4"
    },
    "devDependencies": {
        "@types/plist": "^3.0.2",
        "standard-version": "^9.3.1",
        "typescript": "^4.4.2"
    }
}
